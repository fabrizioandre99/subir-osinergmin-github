<?xml version="1.0" encoding="UTF-8"?>

<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <pluginGroups>
  </pluginGroups>
  <proxies>
  </proxies>
  <servers>
   <server>
      <id>osinergmin-snapshot-repository</id>
      <username>admin</username>
      <password>0sinergnexus</password>
   </server>
  </servers>
  <mirrors>
    <mirror>
      <id>nexus</id>
      <mirrorOf>*,!sonar</mirrorOf>
      <name>Nexus Repository</name>
      <url>http://developer.osinerg.gob.pe/nexus/content/groups/public</url>
    </mirror>
    <mirror>
        <id>nexus-public-snapshots</id>
      <mirrorOf>public-snapshots</mirrorOf>
      <url>http://developer.osinerg.gob.pe/nexus/content/groups/public-snapshots</url>
    </mirror>
  </mirrors>
  <profiles>
   <profile>
      <id>nexus</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://central</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
     <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://central</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>public-snapshots</id>
      <repositories>
        <repository>
          <id>public-snapshots</id>
          <url>http://public-snapshots</url>
          <releases><enabled>false</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
     <pluginRepositories>
        <pluginRepository>
          <id>public-snapshots</id>
          <url>http://public-snapshots</url>
          <releases><enabled>false</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
    <profile>
      <id>osinergmin-snapshots</id>
      <repositories>
        <repository>
          <id>osinergmin-snapshot-repository</id>
	      <name>Osinergmin Snapshot Repository</name>
          <url>http://developer.osinerg.gob.pe/nexus/content/repositories/snapshots/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>osinergmin-snapshot-repository</id>
          <name>Osinergmin Snapshot Repository</name>
          <url>http://developer.osinerg.gob.pe/nexus/content/repositories/snapshots/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
    <activeProfiles>
  	<activeProfile>nexus</activeProfile>
  </activeProfiles>
</settings>
